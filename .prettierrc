{
  "htmlWhitespaceSensitivity": "css",
  "jsxBracketSameLine": false,
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "overrides": [
    {
      "files": "package*.json",
      "options": {
        "printWidth": 1000
      }
    },
    {
      "files": "*.yaml",
      "options": {
        "singleQuote": false,
        "printWidth": 1000
      }
    }
  ]
}
