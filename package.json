{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A very basic starting point to build a react app. ",
  "keywords": [
    "axios",
    "react",
    "parcel",
    "tailwindcss",
    "react-query",
    "react-location",
    "react-hook-form",
    "react-hot-toast",
    "zustand"
  ],
  "source": "index.html",
  "scripts": {
    "start": "NODE_ENV=development parcel --no-cache",
    "prebuild": "rimraf dist",
    "build:dev": "npm run prebuild && NODE_ENV=development parcel build",
    "build:production": "npm run prebuild && NODE_ENV=production parcel build --public-url ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mknospe/react-boilerplate.git"
  },
  "author": "Martin Knospe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mknospe/react-boilerplate/issues"
  },
  "homepage": "https://github.com/mknospe/react-boilerplate#readme",
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@tailwindcss/forms": "^0.4.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.7.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "parcel": "^2.2.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.15"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@hookform/resolvers": "^2.8.5",
    "axios": "^0.24.0",
    "date-fns": "^2.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.24.0",
    "react-hot-toast": "^2.2.0",
    "react-location": "^3.3.0",
    "react-query": "^3.34.8",
    "zod": "^3.11.6",
    "zustand": "^3.6.9"
  },
  "alias": {
    "Core": "./src/modules/core",
    "Images": "./src/images",
    "Libs": "./src/libs",
    "Modules": "./src/modules"
  }
}
